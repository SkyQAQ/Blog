<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <!-- 支付宝网关（固定）http为沙盒环境 -->
  <set name="ALIPAY_URL">
    <![CDATA[http://openapi.alipay.com/gateway.do]]>
  </set>
  <!-- APPID 创建应用后生成 -->
  <set name="ALIPAY_APPID" value="2019040863807290"></set>
  <!-- Rsa256私钥 -->
  <set name="ALIPAY_APP_PRIVATE_KEY" value=""></set>
  <!-- Rsa256公钥 -->
  <set name="ALIPAY_APP_ALIPAY_PUBLIC_KEY" value=""></set>
  <!-- 商户生成签名字符串所使用的签名算法类型 -->
  <set name="ALIPAY_SIGN_TYPE" value="RSA2"></set>
  <!-- 参数返回格式，只支持json -->
  <set name="ALIPAY_FORMAT" value="json"></set>
  <!-- 编码集，支持GBK/UTF-8 -->
  <set name="ALIPAY_CHARSET" value="UTF-8"></set>
</configuration>